int columna = 0;
int celda =0;
float x = 200;
float y = 100;

void setup () {
  size (830, 830);
}

void draw () {
 
  while (celda < 3){
      while (columna < 10){
        fill (#A0ECF0);
        rect (x, y, 100, 20);
        y = y + 20;
        columna += 1;
      }
      x = x+ 200;
      y = 100;
      celda += 1;
      columna = 0;
  }
}
      
